<?xml version="1.0" encoding="UTF-8"?>
<configuration>
	<system.webServer>
		<!-- Sets the Cache-Control header to max-age:30 days -->
		<staticContent>
			<clientCache cacheControlMode="UseMaxAge" cacheControlMaxAge="30.00:00:00"/>
			<mimeMap fileExtension=".webp" mimeType="image/webp"/>
		</staticContent>
		<httpProtocol>
			<customHeaders>
				<add name="Cache-Control" value="public"/>
			</customHeaders>
		</httpProtocol>
	</system.webServer>
</configuration>
